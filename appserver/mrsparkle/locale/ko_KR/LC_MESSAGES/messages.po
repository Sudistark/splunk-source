msgid "<p>$cpuDisplayMsg$</p>"
msgstr "<p>$cpuDisplayMsg$</p>"

#. html
#: splunk_monitoring_console/default/data/ui/views/search_activity_deployment.xml:412 splunk_monitoring_console/default/data/ui/views/search_activity_deployment.xml:484
